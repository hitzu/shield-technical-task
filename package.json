{
  "name": "shield",
  "version": "0.0.1",
  "description": "Shield backend API service",
  "files": [
    "/lib",
    "/types",
    "/static",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "db:start": "docker compose up -d db_postgres",
    "dev": "npm run db:start && npm run start",
    "start": "nodemon index.ts",
    "types": "ttsc --emitDeclarationOnly",
    "build": "rm -rf lib && babel src --out-dir lib --extensions '.ts' && npm run types",
    "test:db:up": "docker compose -f docker-compose.test.yml up -d db_postgres_test",
    "test:db:down": "docker compose -f docker-compose.test.yml down -v",
    "test:ci": "npm run test:db:up && jest && npm run test:db:down",
    "db:stop": "docker compose rm -s -f",
    "migration:run:dev": "PG_HOST=localhost NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:run --dataSource ./src/orm/data-source.ts",
    "migration:revert:dev": "PG_HOST=localhost NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:revert --dataSource ./src/orm/data-source.ts",
    "migration:show:dev": "PG_HOST=localhost NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:show --dataSource ./src/orm/data-source.ts",
    "migration:create": "NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:create ./src/orm/migrations",
    "migration:generate:dev": "PG_HOST=localhost NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:generate ./src/orm/migrations --dataSource ./src/orm/data-source.ts",
    "seed:run:dev": "PG_HOST=localhost NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:run --dataSource ./src/orm/data-source.ts",
    "orm:typeorm:migration": "typeorm migration:create -n ./src/orm/migrations/Migration"
  },
  "license": "public",
  "dependencies": {
    "@types/request-ip": "0.0.37",
    "argon2": "^0.30.3",
    "bcryptjs": "^2.4.3",
    "cerealizr": "^1.0.1-alpha",
    "compression": "^1.7.4",
    "crypto-js": "^4.0.0",
    "custom-env": "^2.0.1",
    "express": "^4.17.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "ip": "^1.1.5",
    "joi": "^17.2.1",
    "joi-extract-type": "^15.0.8",
    "joi-to-swagger": "^5.0.1",
    "json-schema-ref-parser": "^7.1.4",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.7.3",
    "pino": "^9.0.0",
    "pino-http": "^10.2.0",
    "rate-limiter-flexible": "^7.2.0",
    "request-ip": "^2.1.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/jest": "^27.0.1",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.18.36",
    "@types/supertest": "^2.0.11",
    "aws-sdk": "^2.738.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "husky": "^4.2.5",
    "jest": "^27.0.6",
    "lint-staged": "^10.2.2",
    "nodemon": "^1.12.1",
    "prettier": "^1.19.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "ttypescript": "^1.5.10",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^1.1.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{ts}": "tslint --fix",
    "*.{ts,json,md}": "prettier --write"
  },
  "engines": {
    "node": ">=22.14.0 <23"
  }
}
